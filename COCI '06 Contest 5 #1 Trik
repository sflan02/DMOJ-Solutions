moves=input()
location=1
for x in moves:
    if x == "A":
        if location == 1:
            location = 2
        elif location ==2:
            location = 1
    elif x == "B":
        if location == 2:
            location = 3
        elif location ==3:
            location = 2 
    elif x == "C":
        if location == 1:
            location = 3
        elif location ==3:
            location = 1  
print(location)
